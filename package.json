{
  "name": "gaming-platform",
  "version": "0.0.1",
  "main": "./build/app.js",
  "keywords": [
    "api",
    "koa-api"
  ],
  "scripts": {
    "start": "NODE_ENV=production node ./build/app",
    "dev": "ts-node-dev --respawn ./src/app",
    "test": "jest ./tests",
    "ts:build": "tsc",
    "ts:build-ci": "tsc --noEmit",
    "lint": "eslint --max-warnings=0 \"{src,tests}/**/*.{mts,ts}\"",
    "lint-fix": "eslint --fix --max-warnings=0 \"{src,tests}/**/*.{mts,ts}\"",
    "format": "prettier --write \"{src,tests}/**/*.{mts,ts}\"",
    "prepare-dev": "husky install",
    "prepare-typia": "ts-patch install",
    "clean": "rimraf build",
    "bootstrap": "pnpm install && npm run clean && npm run ts:build",
    "prepare": "ts-patch install && typia patch"
  },
  "author": "semori-trade",
  "license": "Private",
  "dependencies": {
    "@koa/bodyparser": "^5.1.1",
    "@koa/router": "^12.0.1",
    "@types/jsonwebtoken": "^9.0.6",
    "argon2": "^0.40.3",
    "better-sqlite3": "^11.1.2",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "kysely": "^0.27.4",
    "pg": "^8.12.0",
    "pino": "^9.1.0",
    "typia": "6.5.4-dev.20240721"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-json": "^2.0.23",
    "@types/koa-logger": "^3.1.5",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.14.0",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^9.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "kysely-codegen": "^0.15.0",
    "lint-staged": "^15.2.5",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "tsimp": "^2.0.11",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.12.0"
  }
}
